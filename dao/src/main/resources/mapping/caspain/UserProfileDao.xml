<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ih2ome.dao.caspain.UserProfileDao">
    <!--根据主键查询用户id-->
    <select id="selectUserIdByPrimaryKey" parameterType="int" resultType="int">
      SELECT
         user_id
      FROM
         user_profile
      WHERE
         id = #{id};
    </select>

</mapper>